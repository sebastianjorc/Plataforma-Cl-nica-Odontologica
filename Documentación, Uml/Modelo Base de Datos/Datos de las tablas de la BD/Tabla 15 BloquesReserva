//Hora;bloque

CREATE TABLE IF NOT EXISTS `Reserva de hora` (
	`HORA` VARCHAR(45) NOT NULL,
	`BLOQUE` INT NULL,
	PRIMARY KEY (`BLOQUE`)
);

