@startuml
	title Casos de Uso: Administrador
	
	actor "Administrador" as user #lightgreen
	
	user -> (Login)
	(Login) --> (Interfaz Administrador):direcciona
	
	(Inventario) <- (Interfaz Administrador):modificar
	(Inventario) --> (Insumos):Administrar
	
	(Interfaz Administrador) --> (Servicios):modificar
	(Servicios) --> (Salud dental)
	(Servicios) --> (Estetica dental)
	(Servicios) --> (Especialidad)
	
	(Interfaz Administrador) -> (Usuarios):modificar
	(Usuarios) --> (Alumno)
	(Usuarios) --> (Paciente)
	(Usuarios) --> (Secretaria)
	(Usuarios) -> (Tesorero)

@enduml