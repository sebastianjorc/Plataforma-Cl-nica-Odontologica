	activate C4 #lightgreen
	C4-> db1:pedir
	activate db1 #yellow
	db1 -->C4:devolver
	deactivate db1
	
	C4 -> db1:guardar cambios
	activate db1 #yellow
	db1 --> C4:Actualizar
	deactivate db1
	
	C4-->C1:volver	
	deactivate C4
	
	C1-->Interfaz:volver
	deactivate C1
	
	
	Interfaz -> A1 :Seleccionar
	activate A1 #lightgreen
	A1 -> db2:pedir
	activate db2 #yellow
	db2 --> A1 :devolver
	deactivate db2
	A1 -> A2:-Agregar\n-Eliminar\n-Modificar
	activate A2 #lightgreen
	activate db2 #yellow
