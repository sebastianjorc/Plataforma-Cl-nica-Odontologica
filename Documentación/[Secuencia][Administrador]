@startuml
	title Diagrama de Secuencia: Administrador __Inventario__\n
	
	legend 
		<size:20><b>Desarollo en verde <color:lightgreen>   █</color>: Interacción usuario-interfaz</b> </size>
		<size:20><b>Desarollo en amarillo <color:yellow>█</color>: procesamiento de datos</b> </size>
		<size:20><b>Desarollo en blanco <color:white>  █</color>: entrega de datos</b> </size>	
	endlegend
	
	actor "Admin" as user #lightgreen
	box "Login" #lightblue
		participant Login
		database Usuarios #yellow
	endbox
	
	box "Interfaz"
		participant "Interfaz Admin" as Interfaz
		participant Inventario as A1
		database Insumos as db1 #yellow
		participant Insumo as A2
	endbox
	
	activate user #lightgreen
	user -> Login :Ingresa
	activate Login #lightgreen
	Login -> Usuarios :Cargar
	activate Usuarios #yellow
	Usuarios -> Interfaz :Direcciona
	deactivate Usuarios
	deactivate Login
	
	activate Interfaz #lightgreen
	Interfaz -> A1 :Seleccionar
	activate A1 #lightgreen
	A1 -> db1:pedir
	activate db1 #yellow
	db1 --> A1 :devolver
	deactivate db1
	A1 -> A2:-Agregar\n-Eliminar\n-Modificar
	activate A2 #lightgreen
	activate db1 #yellow
	A2 --> db1:guardar
	db1 --> A1:actualizar
	deactivate db1
	A2 --> A1:volver
	deactivate A2
	deactivate A1
	A1-->Interfaz:volver	
	
	Interfaz --> Login: Salir
	deactivate Interfaz
	activate Login #Lightgreen
	Login --> "user": Cerrar
	deactivate user
	deactivate Login
@enduml

@startuml
	title Diagrama de Secuencia: Administrador __Servicios__\n
	
	legend 
		<size:20><b>Desarollo en verde <color:lightgreen>   █</color>: Interacción usuario-interfaz</b> </size>
		<size:20><b>Desarollo en amarillo <color:yellow>█</color>: procesamiento de datos</b> </size>
		<size:20><b>Desarollo en blanco <color:white>  █</color>: entrega de datos</b> </size>	
	endlegend
	
	actor "Admin" as user #lightgreen
	box "Login" #lightblue
		participant Login
		database Usuarios #yellow
	endbox
	
	box "Interfaz"
		participant "Interfaz Admin" as Interfaz
		participant Servicios as C1
		participant "Salud dental" as C2
		participant "Estetica dental" as C3	
		participant "Especialidad" as C4
		database Servicios as db1 #yellow
	endbox
	
	activate user #lightgreen
	user -> Login :Ingresa
	activate Login #lightgreen
	Login -> Usuarios :Cargar
	activate Usuarios #yellow
	Usuarios -> Interfaz :Direcciona
	deactivate Usuarios
	deactivate Login
	
	activate Interfaz #lightgreen
	Interfaz -> C1 :Seleccionar
	
	activate C1 #lightgreen
	C1 -> C2:-Agregar\n-Eliminar\n-Modificar	
	activate C2 #lightgreen
	C2-> db1:pedir
	activate db1 #yellow
	db1 -->C2:devolver
	deactivate db1
	
	C2 -> db1:guardar cambios
	activate db1 #yellow
	db1 --> C2:Actualizar
	deactivate db1
	
	C2-->C1:volver	
	deactivate C2
	
	C1 -> C3:-Agregar\n-Eliminar\n-Modificar
	activate C3 #lightgreen
	C3-> db1:pedir
	activate db1 #yellow
	db1 -->C3:devolver
	deactivate db1
	
	C3 -> db1:guardar cambios
	activate db1 #yellow
	db1 --> C3:Actualizar
	deactivate db1
	
	C3-->C1:volver	
	deactivate C3
	
	deactivate C2
	
	C1 -> C4:-Agregar\n-Eliminar\n-Modificar	
	activate C4 #lightgreen
	C4-> db1:pedir
	activate db1 #yellow
	db1 -->C4:devolver
	deactivate db1
	
	C4 -> db1:guardar cambios
	activate db1 #yellow
	db1 --> C4:Actualizar
	deactivate db1
	
	C4-->C1:volver	
	deactivate C4
	
	deactivate C1
	C1-->Interfaz:volver		
	
	Interfaz --> Login: Salir
	deactivate Interfaz
	activate Login #Lightgreen
	Login --> "user": Cerrar
	deactivate user
	deactivate Login
@enduml


@startuml
	title Diagrama de Secuencia: Administrador __Usuarios__\n
	
	legend 
		<size:20><b>Desarollo en verde <color:lightgreen>   █</color>: Interacción usuario-interfaz</b> </size>
		<size:20><b>Desarollo en amarillo <color:yellow>█</color>: procesamiento de datos</b> </size>
		<size:20><b>Desarollo en blanco <color:white>  █</color>: entrega de datos</b> </size>	
	endlegend
	
	actor "Admin" as user #lightgreen
	box "Login" #lightblue
		participant Login
		database Usuarios #yellow
	endbox
	
	box "Interfaz"
		participant "Interfaz Admin" as Interfaz
		participant Usuarios as B1
		participant "Alumno" as B2
		participant "Paciente/\nProfesional" as B3
		participant "Secretaria" as B4
		participant "Tesorero" as B5
	endbox
	
	activate user #lightgreen
	user -> Login :Ingresa
	activate Login #lightgreen
	Login -> Usuarios :Cargar
	activate Usuarios #yellow
	Usuarios -> Interfaz :Direcciona
	deactivate Usuarios
	deactivate Login
	
	activate Interfaz #lightgreen
	Interfaz -> B1 :Seleccionar
	
	activate B1 #lightgreen
	B1 -> B2:-Agregar\n-Eliminar\n-Modificar	
	activate B2 #lightgreen
	B2-> Usuarios:pedir
	activate Usuarios #yellow
	Usuarios -->B2:devolver
	deactivate Usuarios
	
	B2 -> Usuarios:guardar cambios
	activate Usuarios #yellow
	Usuarios --> B2:Actualizar
	deactivate Usuarios
	
	B2-->B1:volver	
	deactivate B2
	
	B1 -> B3:-Agregar\n-Eliminar\n-Modificar
	activate B3 #lightgreen
	B3-> Usuarios:pedir
	activate Usuarios #yellow
	Usuarios -->B3:devolver
	deactivate Usuarios
	
	B3 -> Usuarios:guardar cambios
	activate Usuarios #yellow
	Usuarios --> B3:Actualizar
	deactivate Usuarios
	
	B3-->B1:volver	
	deactivate B3
	
	deactivate B2
	
	B1 -> B4:-Agregar\n-Eliminar\n-Modificar	
	activate B4 #lightgreen
	B4-> Usuarios:pedir
	activate Usuarios #yellow
	Usuarios -->B4:devolver
	deactivate Usuarios
	
	B4 -> Usuarios:guardar cambios
	activate Usuarios #yellow
	Usuarios --> B4:Actualizar
	deactivate Usuarios
	
	B4-->B1:volver	
	deactivate B4
	
	B1 -> B5:-Agregar\n-Eliminar\n-Modificar	
	activate B5 #lightgreen
	B5-> Usuarios:pedir
	activate Usuarios #yellow
	Usuarios -->B5:devolver
	deactivate Usuarios
	
	B5 -> Usuarios:guardar cambios
	activate Usuarios #yellow
	Usuarios --> B5:Actualizar
	deactivate Usuarios
	
	B5-->B1:volver	
	deactivate B5
	
	deactivate B1
	B1-->Interfaz:volver		
	
	Interfaz --> Login: Salir
	deactivate Interfaz
	activate Login #Lightgreen
	Login --> "user": Cerrar
	deactivate user
	deactivate Login
@enduml

@startuml
	title Diagrama de Secuencia: Administrador __General__\n
	
	legend 
		<size:20><b>Desarollo en verde <color:lightgreen>   █</color>: Interacción usuario-interfaz</b> </size>
		<size:20><b>Desarollo en amarillo <color:yellow>█</color>: procesamiento de datos</b> </size>
		<size:20><b>Desarollo en blanco <color:white>  █</color>: entrega de datos</b> </size>	
	endlegend
	
	actor "Admin" as user #lightgreen
	box "Login" #lightblue
		participant Login
		database Usuarios #yellow
	endbox
	
	box "Interfaz"
		participant "Interfaz Admin" as Interfaz
		
		participant Usuarios as B1
		participant "Alumno" as B2
		participant "Paciente/\nProfesional" as B3
		participant "Secretaria" as B4
		participant "Tesorero" as B5
		
		participant Servicios as C1
		participant "Salud dental" as C2
		participant "Estetica dental" as C3	
		participant "Especialidad" as C4
		database Servicios as db1 #yellow
		
		
		participant Inventario as A1
		database Insumos as db2 #yellow
		participant Insumo as A2
		
		
	endbox
	
	activate user #lightgreen
	user -> Login :Ingresa
	activate Login #lightgreen
	Login -> Usuarios :Cargar
	activate Usuarios #yellow
	Usuarios -> Interfaz :Direcciona
	deactivate Usuarios
	deactivate Login
	
	activate Interfaz #lightgreen
	Interfaz -> B1 :Seleccionar
	
	activate B1 #lightgreen
	B1 -> B2:-Agregar\n-Eliminar\n-Modificar	
	activate B2 #lightgreen
	B2-> Usuarios:pedir
	activate Usuarios #yellow
	Usuarios -->B2:devolver
	deactivate Usuarios
	
	B2 -> Usuarios:guardar cambios
	activate Usuarios #yellow
	Usuarios --> B2:Actualizar
	deactivate Usuarios
	
	B2-->B1:volver	
	deactivate B2
	
	B1 -> B3:-Agregar\n-Eliminar\n-Modificar
	activate B3 #lightgreen
	B3-> Usuarios:pedir
	activate Usuarios #yellow
	Usuarios -->B3:devolver
	deactivate Usuarios
	
	B3 -> Usuarios:guardar cambios
	activate Usuarios #yellow
	Usuarios --> B3:Actualizar
	deactivate Usuarios
	
	B3-->B1:volver	
	deactivate B3
	
	deactivate B2
	
	B1 -> B4:-Agregar\n-Eliminar\n-Modificar	
	activate B4 #lightgreen
	B4-> Usuarios:pedir
	activate Usuarios #yellow
	Usuarios -->B4:devolver
	deactivate Usuarios
	
	B4 -> Usuarios:guardar cambios
	activate Usuarios #yellow
	Usuarios --> B4:Actualizar
	deactivate Usuarios
	
	B4-->B1:volver	
	deactivate B4
	
	B1 -> B5:-Agregar\n-Eliminar\n-Modificar	
	activate B5 #lightgreen
	B5-> Usuarios:pedir
	activate Usuarios #yellow
	Usuarios -->B5:devolver
	deactivate Usuarios
	
	B5 -> Usuarios:guardar cambios
	activate Usuarios #yellow
	Usuarios --> B5:Actualizar
	deactivate Usuarios
	
	B5-->B1:volver	
	deactivate B5
	
	deactivate B1
	B1-->Interfaz:volver		
	
		Interfaz -> C1 :Seleccionar
	
	activate C1 #lightgreen
	C1 -> C2:-Agregar\n-Eliminar\n-Modificar	
	activate C2 #lightgreen
	C2-> db1:pedir
	activate db1 #yellow
	db1 -->C2:devolver
	deactivate db1
	
	C2 -> db1:guardar cambios
	activate db1 #yellow
	db1 --> C2:Actualizar
	deactivate db1
	
	C2-->C1:volver	
	deactivate C2
	
	C1 -> C3:-Agregar\n-Eliminar\n-Modificar
	activate C3 #lightgreen
	C3-> db1:pedir
	activate db1 #yellow
	db1 -->C3:devolver
	deactivate db1
	
	C3 -> db1:guardar cambios
	activate db1 #yellow
	db1 --> C3:Actualizar
	deactivate db1
	
	C3-->C1:volver	
	deactivate C3
	
	deactivate C2
	
